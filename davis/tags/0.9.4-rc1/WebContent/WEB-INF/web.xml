</servlet>
<servlet>
	<servlet-name>quickshare</servlet-name>
	<servlet-class>webdavis.quickshare.QuickShare</servlet-class>
	<init-param>
		<description>
			Configuration files to be read (in order)
		</description>
		<param-name>config-files</param-name>
		<param-value>davis.properties, davis-organisation.properties, davis-host.properties, davis-dev.properties</param-value>
	</init-param>
</servlet>
<servlet-mapping>
	<servlet-name>Davis</servlet-name>
	<url-pattern>/*</url-pattern>
</servlet-mapping>
<servlet-mapping>
	<servlet-name>quickshare</servlet-name>
	<url-pattern>/quickshare/*</url-pattern>
</servlet-mapping>
<listener>
	<listener-class>webdavis.DavisListener</listener-class>
</listener>
<session-config>
   	<session-timeout>30</session-timeout>
</session-config>
</web-app>
